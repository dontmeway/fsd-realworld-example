{
  "name": "medhub-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "env-cmd -e dev vite",
    "start:prod": "env-cmd -e prod vite",
    "build:dev": "env-cmd -e dev tsc && vite build",
    "build:prod": "env-cmd -e prod tsc && vite build",
    "preview:prod": "env-cmd -e prod vite preview",
    "release": "release-it",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "lint": "tsc --noEmit && eslint 'src/**/*.ts{,x}' --fix",
    "test": "vite-jest --no-cache"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.8.6",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "browserslist": "^4.20.0",
    "compose-function": "^3.0.3",
    "dayjs": "^1.10.8",
    "effector": "^22.2.0",
    "effector-react": "^22.0.6",
    "env-cmd": "^10.1.0",
    "framer-motion": "^6",
    "i18next": "^21.6.12",
    "query-string": "^7.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.15.5",
    "react-router-dom": "^6.2.2",
    "typescript": "^4.6.2",
    "vite": "^2.8.6",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^4.2.2",
    "@types/compose-function": "^0.0.30",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@vitejs/plugin-legacy": "^1.7.1",
    "@vitejs/plugin-react": "^1.2.0",
    "eslint-config-airbnb-typescript-prettier": "^5.0.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-effector": "^0.6.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "release-it": "^14.12.5",
    "source-map-explorer": "^2.5.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "vite-jest": "^0.1.4",
    "vite-tsconfig-paths": "^3.4.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
